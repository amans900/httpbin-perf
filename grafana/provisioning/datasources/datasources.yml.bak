apiVersion: 1
datasources:
  - name: InfluxDB-jmeter
    type: influxdb
    access: proxy
    url: http://influxdb:8086
    database: jmeter
    isDefault: true
  - name: Prometheus
    type: prometheus
    access: proxy
    url: http://prometheus:9090
  - name: Loki
    type: loki
    access: proxy
    url: http://loki:3100